                        <div class="input-group input-group-merge">
                            <span id="basic-icon-default-phone2" class="input-group-text"><i class="icon-base bx bx-user-pin"></i></span>
                            <input class="form-control" type="text" id="YakinAdSoyad" name="YakinAdSoyad" autofocus="" value="@Model.YakinAdSoyad">
                        </div>
                    </div>
                    <div class="col-md-6">
                        @Html.LabelFor(model => model.YakinTCNO, htmlAttributes: new { @class = "control-label col-md-6" })
                        <div class="input-group input-group-merge">
                            <span id="basic-icon-default-phone2" class="input-group-text"><i class="icon-base bx bx-id-card"></i></span>
                            <input class="form-control" type="text" id="YakinTCNO" name="YakinTCNO" autofocus="" value="@Model.YakinTCNO">
                        </div>
                    </div>
                    <div class="col-md-6">
                        @Html.LabelFor(model => model.YakinTelNo, htmlAttributes: new { @class = "control-label col-md-6" })
                        <div class="input-group input-group-merge">
                            <span id="basic-icon-default-phone2" class="input-group-text"><i class="icon-base bx bx-phone-call"></i></span>
                            <input class="form-control" type="text" id="YakinTelNo" name="YakinTelNo" autofocus="" value="@Model.YakinTelNo">
                        </div>
                    </div>
                    <div class="mt-6">
                        <button type="submit" class="btn btn-primary me-3">Personeli Düzenle</button>
                        <a href="/PersonelBilgileris/Index/" type="reset" class="btn btn-outline-secondary">Personel Listesine Dön</a>
                    </div>
                </div>
            </form>
        </div>
    </div>

</div>
}