@model ProjeTakipSitesi.Models.Personel.PersonelBilgileri

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
<div class="form-horizontal">

    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.PersonelBilgileriId)

    <div class="card mb-6">
        <h5 class="card-header">PERSONEL EKLE</h5>
        <div class="card-body pt-4">
            <form id="formAccountSettings" method="POST" onsubmit="return false">
                <div class="row g-6">
                    <div class="col-md-6">
                        @Html.LabelFor(model => model.PersonelGorseli, htmlAttributes: new { @class = "control-label col-md-6" })
                        <div class="input-group input-group-merge">
                            <span id="basic-icon-default-fullname2" class="input-group-text"><i class="icon-base bx bx-image-alt"></i></span>
                            <input class="form-control" type="text" id="PersonelGorseli" name="PersonelGorseli" autofocus="" value="@Model.PersonelGorseli">
                        </div>
                    </div>
                    <div class="col-md-6">
                        @Html.LabelFor(model => model.AdSoyad, htmlAttributes: new { @class = "control-label col-md-6" })
                        <div class="input-group input-group-merge">
                            <span id="basic-icon-default-fullname2" class="input-group-text"><i class="icon-base bx bx-user"></i></span>
                            <input class="form-control" type="text" id="AdSoyad" name="AdSoyad" autofocus="" value="@Model.AdSoyad">
                        </div>
                    </div>
                    <div class="col-md-6">
                        @Html.LabelFor(model => model.Eposta, htmlAttributes: new { @class = "control-label col-md-6" })
                        <div class="input-group input-group-merge">
                            <span class="input-group-text"><i class="icon-base bx bx-envelope"></i></span>
                            <input class="form-control" type="email" name="Eposta" id="Eposta" value="@Model.Eposta">
                        </div>
                    </div>
                    <div class="col-md-6">
                        @Html.LabelFor(model => model.Departman, htmlAttributes: new { @class = "control-label col-md-6" })
                        <div class="input-group input-group-merge">
                            <span id="basic-icon-default-company2" class="input-group-text"><i class="icon-base bx bx-buildings"></i></span>
                            <input class="form-control" type="text" name="Departman" id="Departman" value="@Model.Departman">
                        </div>
                    </div>
                    <div class="col-md-6">
                        @Html.LabelFor(model => model.Gorev, htmlAttributes: new { @class = "control-label col-md-6" })
                        <div class="input-group input-group-merge">
                            <span id="basic-icon-default-company2" class="input-group-text"><i class="icon-base bx bx-briefcase"></i></span>
                            <input class="form-control" type="text" name="Gorev" id="Gorev" value="@Model.Gorev">

                        </div>
                    </div>
                    <div class="col-md-6">
                        @Html.LabelFor(model => model.TCNO, htmlAttributes: new { @class = "control-label col-md-6" })
                        <div class="input-group input-group-merge">
                            <span id="basic-icon-default-company2" class="input-group-text"><i class="icon-base bx bx-id-card"></i></span>
                            <input class="form-control" type="text" id="TCNO" name="TCNO" autofocus="" value="@Model.TCNO">
                        </div>
                    </div>
                    <div class="col-md-6">
